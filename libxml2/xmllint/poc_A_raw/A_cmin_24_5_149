<!DOCTYPE ksiazka [
<!ELEMENT ksiazka (kontakt*,grupa+)>
<!ENTITY dr  "doktor">
<!ELEMENT kontakt (dane_personalne,adres+,dane_kontaktowe,daty,strDom)>
<!ATTLIST kontakt plec (m|k|brak) #REQUIRED
				  id_grupa IDREF #REQUIRED>
<!ELEMENT dane_personalne (tytul?,imie,nazwisko)>
<!ELEMENT dytul (#PCDATA)>
<!ELEMENT imie (#PCDATA)>
<!ELEMENT nazwisko (#PCDATA)>
<!ELEMENT dane_kontaktowe (tel,email,gg?,status)>
<!ELEMENT tel (#PCDATA)>




<!ELEMENT adres (kod_pocztowy,miejscowosc,ulica,nr_budynku,nr_lokalu?)>
<!ATTLIST adres rodzaj CDATA #IMPLIED>
<!ELEMENT kod_pocztowy (#PCDATA)>
<!ELEMENT miejscowosc (#PCDATA)>
<!ELEMENT ulica (#PCDATA)>
<!ELEMENT nr_budynku (#PCDATA)>
<!ELEMENT nr_lokalu (#PCDATA)>

<!ELEMENT gg (#PCDATA)>
<!ELEMENT email (#PCDATA)>
<!ATTLIST email domena NMTOKENS #IMPLIED>
<!ATTLIST status tekst NMTOKENS #REQUIRED>
<!ELEMENT daty (#PCDATA)>
<!ELEMENT strDom (#PCDATA)>


<!ELEMENT grupa (#PCDATA)>
<!ATTLIST grupa  id ID #REQUIRED>
]>





