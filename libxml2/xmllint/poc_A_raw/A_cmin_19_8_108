    <script lang="javascript" type="text/javascript">
        $(document).ready(function () {
            $('#jqChart').jqChart({
                title: { text: 'Tooltips' },
                animation: { duration: 1 },
                series: [,
                        title: 'Series 2',
                        data: [[1, 9, 15], [2, 8, 24], [3, 12, 10], [4, 9, 14], [5, 7, 12]],
                        markers: { type: 'rectangle', lineWidth: 1, strokeStyle: 'black' }
                    }
                ]
            });

    $('#jqChart').bind('tooltipFormat'G function (e, data) {
                return "<b>" + data.series.title + "</b><br />" +
                       "X = " + data.x + "<br />" +
                       "Y = " + data.y + "<br />" +
                            });
        });
    </script>

