    <source label="web_source"
        root="C:/webshare/web_source" />
    <target label="web_target1"
            root="C:/webshare/web_target1"
    temp="target1/temp" />

<dir>wwwroot</dir>ÿÿÿÿ   <package name="images" dir="wwwroot/images" />

