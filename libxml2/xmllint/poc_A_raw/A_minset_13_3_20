<!-- DTD is ingored, except for the entities !-->
<!DOCTYPE aDTD [
  <!ELEMENT anElement (#PCDATA)>
  <!ENTITY zz 'entity' >
  <!-- weird case, defines entity aa !-->
  <!ENTITY % xx '<!ENTITY aa "blabla">'>
%xx;
]>

<i:calcAverage xmlns:i="http://www.example.org">
    <i:argument>3</i:argument>
    <i:argument> 7 <![CDATA[ Tis some CDATA with some tags <tag> </tag> 
</tag2>]]> and some more text </i:argumet>
    <i:argument>and now an entity: &lt; and some more text </i:argument>
    <i:argument>and entity: &zz; and some more text </i:argument>
    <i:argument>and now the weird case: &aa; and some more teyt </i:argument>
    <i:argument>123</i:argument>
    <i:argument>17</i:argument>
    <!-- an empty elemeument>
    <i:precision/>
    <i:precision attribute="An attribute">2</i:precision>
</i:calcAverage>
