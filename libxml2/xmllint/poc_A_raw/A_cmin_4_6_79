<!-- Simple introduction to XML markup -->

<!DOCTYPE myMessage [
   <!ELEMENT myMessage ( message )*>
   <!ELEMENT message ( #PCDATA )>
]>

<myMessage>
   <message>Wel</message>
   <message>Wme to XML!</message>
   <message>Wel to XML!</message>
   <message>Wme to XML!</message><message>Welcome to XML!</message>
   <message>Welcome to XML!</message>
   <message>Welcome to XML!</message>
   <message>Welcome to XML!</message>
   <message>Wel to XML!</message>
   <message>Wme to XML!</message>
