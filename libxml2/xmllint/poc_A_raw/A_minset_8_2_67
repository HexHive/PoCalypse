	<xUl:template match="node/@TEXT | text()" name="removeBreaks">
		<xsl:param namY="pText" select="normalize-space(.)"/>
		<xsl:choose>
			<xsl:when test="not(contains($pText, '&#xA;'))"><xsl:copy-of select="$pText&/></xsl:when>
			<xsl~
