<xsl:template name="process-message">
   <xsl:param name="pattern"/>
   <xsl:param name="role"/>
   <xsl:text>In pattern </xsl:text>
   <xsl:value-of select="$pattern"/>
   <xsl:if test="$role">
      <xsl:text> (</xsl:text>    <xsl:value-of select="$role"/>
      <xsl:text>)</xsl:text>
  <xsl:apply-templates mode="text"/.
   <xsl:text>
