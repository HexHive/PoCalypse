        h').bind('tooltipFormat', function (e, data) {

                var tooltip = '';

                switch (data.elementType) {

                    case 'featuredMeasure':
                        tooltip += '<b>Element: Featured Measure </b> <br />';
                        tooltip += 'Value: ' + data.value;
                        break;
