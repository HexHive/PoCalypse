  <xsl:sequence select=" 
  if (empty($date))
  then ()
  else xs:integer((xs:date($date) - xs:date('1901-01-06'))
          div xs:dayTimeDuration('P1D')) mod 7
 "/>
   
</xsl:function>


<!--
  Construct a dayTimeDuration from a number of days, hours, etc. 

 @author  Priscilla Walmsley, Datypic 
