<section>7.1.2</section>
<section>7</section>
<section>4.12</section>
<documentation>The group element is normalized out.</documentation>
<correct>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
<start>
  <element name="foo">
    <oneOrMore>
      <group>
        <attribute>
          <anyName/>
        </attribute>
      </group>
    </oneOrMore>
  </element>
</start>
</grammar>
</correct>
<valid>
<foo xyzzy1="val1" xyzzy2="val2"/>
</valid>
</testCase>
<testCase>
<section>7.1.2</section>
<section>7</section>
<section>4.21</section>
<documentation>The group element is normalized out.</documentation>
<correct>
<grammar xmlns="http://relaxng.org/ns/structure/1.0">
<start>
  <element name="foo">
    <oneOrMore>
      <group>
        <attribute>
          <anyName/>
        </attribute>
        <empty/>
      </group>
    </oneOrMore>
  </element>
</start>
</grammar>
</correct>
