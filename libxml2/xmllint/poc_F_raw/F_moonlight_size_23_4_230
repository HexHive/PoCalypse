        <text/>
      </attribute>
      <attribute name="bar2">
        <text/>
      </attribute>
    </group>
    <group>
      <attribute name="bar1">
        <text/>
      </attribute>
      <attribute name="bar3">
        <text/>
      </attribute>
    </group>
  </choice>
</element>
</correct>
