		<!-- displayLabel added to <subject> in 3.4. -->
		<xs:attribute name="altRepGroup" type="xs:string"/>
		<!--  altRepGroup new in 3.4 -->
		<xs:attribute name="usage" type="usageAttributeDefinition"/>
		<!--  usage added to <subject>  in 3.4 -->
	</xs:complexType>
