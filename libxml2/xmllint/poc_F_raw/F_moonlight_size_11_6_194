      <value type="string">x</value>
      <value type="string">y</value>
    </group>
  </list>
</element>
</correct>
<valid>
<foo>x y</foo>
</valid>
<valid>
<foo>x  y</foo>
</valid>
<valid>
<foo>&#xD;x&#xA;y&#x9;</foo>
</valid>
<invalid>
<foo>x</foo>
</invalid>
</testCase>
