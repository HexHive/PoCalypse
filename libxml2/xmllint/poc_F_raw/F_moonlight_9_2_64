      <start>
        <ref name="foo"/>
      </start>
      <define name="foo">
        <element name="innerFoo">
           <parentRef name="foo"/>
        </element>
      </define>
    </grammar>
  </start>
  <define name="foo">
    <element name="outerFoo">
      <empty/>
    </element>
  </define>
</grammar>
</correct>
<valid><innerFoo><outerFoo/></innerFoo></valid>
<invalid><outerFoo/></invalid>
</testCase>
