  %brandDTDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDDapps/lvcale/pl.dtd">
  <!ENTITY % brandDTD SYSTEM "chroins/plugins.dtd">
  <!ENTITY % globalDTD SYSTEM "ce://global/l">
  <!ENTITY % brandDTD SYSTEM "chrome://brocale/brand.dtd" >
  %pluginsDTD;
  %globalDTD;
  %br  <!ENTITY % gM "c%globalDTD;
  %bfandDTD;
